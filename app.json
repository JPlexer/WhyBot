{
  "name": "WhyBot",
  "description": "A Fun Discord Bot",
  "repository": "https://github.com/JPlexer/WhyBot/",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "keywords": ["bot", "heroku", "music-playback", "discord", "javascript", "node", "youtube"],
  "env": {
    "BOT_TOKEN": {
      "description": "Discord token used to log in",
      "required": true
    },
    "CL_TOKEN": {
      "description": "Cleverbot io API Key",
      "required": true
    },
        "CL_USER": {
      "description": "Cleverbot io API User",
      "required": true
    },
    "YT_TOKEN": {
      "description": "Google developers project API key",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs"
    },
    {
      "url": "https://github.com/issueapp/heroku-buildpack-ffmpeg"
    }
  ]
}
